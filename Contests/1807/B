#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int m=0,b=0;
        vector<int>vec(n);
        for(int i=0;i<n;i++) cin>>vec[i];
        for(int i=0;i<n;i++)
        {
            if(vec[i]%2==0) m+=vec[i];
            else b+=vec[i];
        }
        if(m>b) cout<<"Yes"<<endl;
        else cout<<"No"<<endl;

    }
    return 0;
}
